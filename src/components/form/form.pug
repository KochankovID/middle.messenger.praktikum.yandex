mixin form(title)
  div.form-mixin
    if block
      .form-block
        block
    script(type="module").
      import Form from "/src/components/form/form.ts";
      let formElement = (new Form({
      title: "#{title}",
      }))
      formElement.setInnerBlock(document.currentScript.parentNode.firstChild.innerHTML)
      document.currentScript.parentNode.innerHTML += formElement.element
