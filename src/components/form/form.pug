mixin form(title)
  - const id = Math.random() * 100000;
  div.form-mixin
    if block
      .form-block
        block
    script(id=id, type="module").
      import Form from "/src/components/form/form.ts";
      let formElement = (new Form({
      title: "#{title}",
      }))
      formElement.setInnerBlock(document.currentScript.parentNode.firstChild.innerHTML)
      document.getElementById("#{id}").parentNode.innerHTML += formElement.element
