mixin link(url)
  - const id = Math.random() * 100000;
  div.link-mixin
    if block
      .link-block
        block
    script(id=id, type="module").
      import Link from "/src/components/link/link.ts";
      let linkElement = (new Link({
      url: "#{url}",
      }))
      linkElement.setInnerBlock(document.currentScript.parentNode.firstChild.innerHTML)
      document.getElementById("#{id}").parentNode.innerHTML += linkElement.element
